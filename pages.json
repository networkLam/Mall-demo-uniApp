			},
			{
				"text": "购物车",
				"iconPath": "./static/icon/car.png",
				"selectedIconPath": "./static/icon/car-active.png",
				"pagePath": "pages/shoppingCar/shoppingCar"
			},
			{
				"text": "我",
				"pagePath": "pages/info/info",
