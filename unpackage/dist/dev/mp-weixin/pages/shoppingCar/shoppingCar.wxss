.wrapper {
  width: 100%;
}
.content {
  background-color: white;
  margin: 12rpx 0rpx 10rpx 0rpx;
  display: flex;
  width: 100%;
  padding-bottom: 96rpx;
}
.content .checkboxLocation {
  display: flex;
  align-items: center;
  width: 10%;
}
.content .describe {
  display: flex;
  align-items: center;
  width: 90%;
}
.content .describe .imgwidth {
  width: 30%;
}
.content .describe .textWidth {
  width: 70%;
}
.content .describe .textWidth .productName {
  width: 100%;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  margin-left: 12rpx;
  margin-right: 12rpx;
  font-size: 32rpx;
}
.content .describe .textWidth .productPrice {
  width: 100%;
  font-size: 28rpx;
  display: flex;
  justify-content: space-between;
}
.content .describe .textWidth .productPrice text:nth-child(1) {
  margin-left: 40rpx;
  margin-top: 40rpx;
  color: black;
  font-size: 26rpx;
}
.content .describe .textWidth .productPrice text:nth-child(2) {
  color: orange;
  font-size: 36rpx;
  margin-right: 22rpx;
  margin-top: 28rpx;
}
.carsclear {
  text-align: center;
  font-size: 36rpx;
  color: orange;
  line-height: 223rpx;
}
.car {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  height: 90rpx;
  background-color: #f3f3f3;
  position: fixed;
  bottom: 100rpx;
  left: 0;
}
.car .checkall text {
  font-size: 26rpx;
  color: black;
}
.car .checkall .cheboxStyle {
  margin-left: 22rpx;
  border-radius: 24px;
}
.car .submit {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.car .submit text:nth-child(1) {
  font-size: 26rpx ;
}
.car .submit text:nth-child(2) {
  color: orange;
  font-size: 32rpx;
}
.car .submit text:nth-child(3) {
  color: orange;
  font-size: 36rpx;
  margin-right: 22rpx;
}
.car .submit button {
  border-radius: 36rpx;
  background-color: orange;
  color: white;
  margin-right: 22rpx;
}

