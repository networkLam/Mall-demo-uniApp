.seting {
  margin-bottom: 22rpx;
}
.seting .seting_icon {
  display: flex;
  justify-content: flex-end;
}
.seting .seting_icon image {
  margin-top: 12rpx;
  margin-right: 12rpx;
  width: 50rpx;
  height: 50rpx;
}
.wrapper_login {
  display: flex;
  justify-content: center;
  align-items: center;
}
.wrapper_login .login image {
  width: 90px;
  height: 90px;
  border-radius: 50%;
  background-color: pink;
}
.list {
  margin-top: 170rpx;
}
.list ._ul {
  list-style: none;
  margin: 0;
  padding: 0;
}
.list ._ul ._li {
  margin-top: 12rpx;
  padding-left: 16rpx;
  font-size: 36rpx;
  font-weight: 500;
  color: black;
  border-top: 1px solid rgba(0, 0, 0, 0.2);
  display: flex;
  align-items: center;
}
.list ._ul ._li image {
  width: 50rpx;
  height: 50rpx;
  margin-right: 12rpx;
}

